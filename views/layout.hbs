  <head>
    <meta charset="utf-8">
    <title>Starshard{{#if title}} - {{/if}}{{title}}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{{pkg.description}}">
    <meta name="author" content="{{pkg.author}}">

    <link href="/css/bootstrap{{#if production}}.min{{/if}}.css" rel="stylesheet">
    <link href="/css/style{{#if production}}.min{{/if}}.css" rel="stylesheet">

    <!-- Favicons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="/img/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="/img/ico/apple-touch-icon-114-precomposed.png">
      <link rel="apple-touch-icon-precomposed" sizes="72x72" href="/img/ico/apple-touch-icon-72-precomposed.png">
                    <link rel="apple-touch-icon-precomposed" href="/img/ico/apple-touch-icon-57-precomposed.png">
                                   <link rel="shortcut icon" href="/img/ico/favicon.png">

    <!-- Place anything custom after this. -->
  </head>
  <body ng-app="starshard">
    <!--[if lt IE 9]>
    <script src="/components/es5-shim/es5-shim.js"></script>
    <script src="/components/json3/lib/json3.min.js"></script>
    <![endif]-->

    <script src="/components/angular/angular{{#if production}}.min{{/if}}.js"></script>
    <script src="/components/angular-resource/angular-resource{{#if production}}.min{{/if}}.js"></script>
    <script src="/components/angular-cookies/angular-cookies{{#if production}}.min{{/if}}.js"></script>
    <script src="/components/angular-sanitize/angular-sanitize{{#if production}}.min{{/if}}.js"></script>
    <script src="/components/angular-route/angular-route{{#if production}}.min{{/if}}.js"></script>
    <script src="/components/angular-bootstrap/ui-bootstrap-tpls{{#if production}}.min{{/if}}.js"></script>
    <script src="/components/ngprogress/build/ngProgress{{#if production}}.min{{/if}}.js"></script>
    <script src="/components/angular-file-upload/angular-file-upload{{#if production}}.min{{/if}}.js"></script>

    <script src="/js/application/application{{#if production}}.min{{/if}}.js"></script>
    <script src="/js/application/controllers{{#if production}}.min{{/if}}.js"></script>

    <!-- Google Analytics: change UA-XXXXX-X to be your site's ID -->
